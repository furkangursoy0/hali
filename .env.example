# Frontend runtime environment
EXPO_PUBLIC_APP_ENV=development

# Backend API base URL
EXPO_PUBLIC_API_BASE_URL=http://localhost:8787

# Frontend mock daily render limit
EXPO_PUBLIC_DAILY_RENDER_LIMIT=20

# false: use local mock, true: call backend /api/usage endpoints
EXPO_PUBLIC_USE_BACKEND_LIMIT=false

# Bulut gorsel modu: mock | cloudinary-unsigned
EXPO_PUBLIC_STORAGE_MODE=mock

# Cloudinary unsigned upload ayarlari (storage_mode cloudinary-unsigned iken gerekli)
EXPO_PUBLIC_CLOUDINARY_CLOUD_NAME=
EXPO_PUBLIC_CLOUDINARY_UPLOAD_PRESET=
